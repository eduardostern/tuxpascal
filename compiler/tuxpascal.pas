  { ldur d0, [x29, #-32] }
  writeln('    ldur d0, [x29, #-32]');
  writeln('    fmul d3, d3, d0');
  EmitMovX0(6);
  EmitScvtfD0X0;
  writeln('    fdiv d3, d3, d0');
  EmitLdurD0(-40);
  writeln('    fadd d0, d0, d3');
  EmitSturD0(-40);

  { Term 8: r/5040 }
  { ldur d0, [x29, #-32] }
  writeln('    ldur d0, [x29, #-32]');
  writeln('    fmul d3, d3, d0');
  EmitMovX0(7);
  EmitScvtfD0X0;
  writeln('    fdiv d3, d3, d0');
  EmitLdurD0(-40);
  writeln('    fadd d0, d0, d3');
  EmitSturD0(-40);

