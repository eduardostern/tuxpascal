procedure EmitMovX0(val: integer);
var
  lo, hi: integer;
  neg: integer;
begin
  neg := 0;
  if val < 0 then
  begin
    neg := 1;
    val := 0 - val
  end;
  if val > 65535 then
  begin
    lo := val mod 65536;
    hi := val div 65536;
    write('    movz x0, #'); writeln(lo);
    write('    movk x0, #'); write(hi); writeln(', lsl #16')
  end
  else
  begin
    write('    mov x0, #'); writeln(val)
  end;
  if neg = 1 then
    writeln('    neg x0, x0')
end;

procedure EmitMovX16(val: integer);
